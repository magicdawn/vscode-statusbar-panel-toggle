{
  "name": "panel-toggle",
  "displayName": "panel-toggle",
  "description": "toggle pabel visibility in your vscode status bar",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.81.0"
  },
  "publisher": "magicdawn",
  "repository": "https://github.com/magicdawn/vscode-statusbar-panel-toggle",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "do-package": "vsce package --no-dependencies",
    "do-publish": "vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
